<!-- Hero Section Begin -->
<section class="hero-section">
    <div class="hero-items owl-carousel">
        <% result.forEach(function(row) { %>
            <div class="single-hero-item set-bg" data-setbg="img/<%= row.feature_image %>">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-8 offset-lg-2">
                            <div class="hero-text">
                                <p class="room-location"><i class="icon_pin"></i> <%= row.location %></p>
                                <h2><%= row.small_desc %></h2>
                                <div class="room-price">
                                    <span><%= __('start_from') %></span>
                                    <p>$<%= row.price %></p>
                                </div>
                                <ul class="room-features">
                                    <li>
                                        <i class="fa fa-arrows"></i>
                                        <p><%= row.space %></p>
                                    </li>
                                    <li>
                                        <i class="fa fa-bed"></i>
                                        <p><%= row.rooms %> <%= __('bed_room') %></p>
                                    </li>
                                    <li>
                                        <i class="fa fa-bath"></i>
                                        <p><%= row.bath %> <%= __('baths_bed') %></p>
                                    </li>
                                    <li>
                                        <i class="fa fa-car"></i>
                                        <p><%= row.garage %> <%= __('garage') %></p>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        <% }) %>
    </div>
    <div class="thumbnail-pic">
        <div class="thumbs owl-carousel">
            <% result.forEach(function(row) { %>
                <div class="item">
                    <img src="img/<%= row.feature_image %>" alt="">
                </div>
            <% }) %>
            
        </div>
    </div>
</section>
<!-- Hero Section End -->

<!-- Search Form Section Begin -->
<div class="search-form">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="search-form-text">
                    <div class="search-text">
                        <i class="fa fa-search"></i>
                        <%= __('find_your_home') %>
                    </div>
                    <div class="home-text">
                        <i class="fa fa-home"></i>
                        <%= __('house_for_sell') %>
                    </div>
                </div>
                <form id="search" action="/search" method="POST" class="filter-form">
                    <div class="first-row row">
                        <select name="type">
                            <option value="selected disabled"><%= __('property_type') %></option>
                            <option value="villa"><%= __('villa') %></option>
                            <option value="townHouse"><%= __('town_house') %></option>
                            <option value="apartment"><%= __('apartment') %></option>
                        </select>
                        <select name="room">
                            <option value="selected disabled"><%= __('bedroom') %></option>
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                            <option value="6">6+</option>
                        </select>
                        <select name="bath">
                            <option value="selected disabled"><%= __('bathroom') %></option>
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                            <option value="6">6+</option>
                        </select>
                        <select name="furniture_type">
                            <option value="selected disabled>"><%= __('furniture_type') %></option>
                            <option value="villa"><%= __('unfurnished') %></option>
                            <option value="townHose"><%= __('furnished') %></option>
                            <option value="apartment"><%= __('semi_furnished') %></option>
                        </select>
                    </div>
                    <div class="second-row row">
                        <div class="price-range-wrap">
                            <div class="price-text">
                                <label for="priceRange"><%= __('price') %>:</label>
                                <input type="text" name="price" id="priceRange" readonly>
                            </div>
                            <div id="price-range" class="slider"></div>
                        </div>
                        <div class="room-size-range">
                            <div class="price-text">
                                <label for="roomsizeRange"><%= __('size') %>:</label>
                                <input type="text" name="space" id="roomsizeRange" readonly>
                            </div>
                            <div id="roomsize-range" class="slider"></div>
                        </div>
                        <input type="submit" value="<%= __('search') %>" class="search-btn" />
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- Search Form Section End -->

